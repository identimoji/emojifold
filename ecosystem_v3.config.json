{
  "apps": [
    {
      "name": "emojifold-manifold-discovery",
      "script": "python",
      "args": "emojifold/batch_v3.py",
      "cwd": "/Users/rob/repos/emojifold",
      "instances": 1,
      "exec_mode": "fork",
      "watch": false,
      "max_memory_restart": "8G",
